		<part name="id" desc="数据点ID">
			<type>aptotic</type>
			<value>08020fff</value>
			<class>byte</class>
			<len>4</len>
		</part>
		<part name="enum_state" desc="告警状态">
			<type>random</type>
			<value>00000001,00000002,00000003</value>
			<class>byte</class>
			<len>4</len>
		</part>
		<part name="content" desc="业务数据内容">
			<type>generate</type>
			<value>
				<part name="content-header" desc="业务数据头">
					<type>aptotic</type>
					<value>[</value>
					<split></split>
					<fill-byte></fill-byte>
					<fill-direction></fill-direction>
					<class>asc</class>
					<len></len>
				</part>
				<part name="body" desc="业务数据具体内容">
					<type>generate</type>
					<value>
						<part name="object_name" desc="对象名称">
							<type>aptotic</type>
							<value>X轴倾斜角</value>
							<split></split>
							<fill-byte>20</fill-byte>
							<fill-direction>right</fill-direction>
							<class>asc</class>
							<len>42</len>
						</part>
						<part name="id_desc" desc="数据点ID">
							<type>aptotic</type>
							<value>08020fff</value>
							<split></split>
							<fill-byte></fill-byte>
							<fill-direction></fill-direction>
							<class>byte</class>
							<len>4</len>
						</part>
						<part name="alarm_level" desc="告警级别">
							<type>random</type>
							<value>紧急,重要,一般,正常</value>
							<split></split>
							<fill-byte></fill-byte>
							<fill-direction></fill-direction>
							<class>asc</class>
							<len></len>
						</part>
						<part name="alarm_text" desc="告警文本">
							<type>aptotic</type>
							<value>12.34</value>
							<split></split>
							<fill-byte>20</fill-byte>
							<fill-direction>right</fill-direction>
							<class>float</class>
							<len>32</len>
						</part>
					</value>
					<split>tab</split>
					<fill-byte></fill-byte>
					<fill-direction></fill-direction>
					<class></class>
					<len></len>
				</part>
				<part name="content-tail" desc="业务数据尾">
					<type>aptotic</type>
					<value>]</value>
					<split></split>
					<fill-byte></fill-byte>
					<fill-direction></fill-direction>
					<class>asc</class>
					<len></len>
				</part>
			</value>
			<class>asc</class>
			<len></len>
		</part>
		<part name="check" desc="校验码">
			<type>check</type>
			<value>
				<part name="check-info" desc="长度+报文序号+命令字+内容">
					<type>crc</type>
					<value>length,serials_no,pk_type,cnt,id,enum_state,content</value>
					<split>,</split>
					<fill-byte></fill-byte>
					<fill-direction></fill-direction>
					<class></class>
					<len></len>
				</part>
			</value>
			<class>byte</class>
			<len>1</len>
