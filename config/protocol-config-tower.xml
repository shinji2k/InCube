					<value>length,serials_no,pk_type,cnt,id,enum_state,content</value>
					<split>,</split>
					<fill-byte></fill-byte>
					<fill-direction></fill-direction>
					<class>asc</class>
					<len>4</len>
				</part>
			</value>
			<class>asc</class>
			<len>4</len>
		</part>
		<part name="serials_no" desc="报文序号">
